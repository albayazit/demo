;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	20.20.20.in-addr-arpa. root.20.20.20.in-addr.arpa. (
			     10		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	20.20.20.in-addr-arpa.
@	IN	A	20.20.20.2
2	IN	PTR	hq-srv.hq.work.
1	IN	PTR	hq-r.hq.work.